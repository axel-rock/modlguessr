{
  "name": "modlguessr",
  "author": "Axel Rock",
  "version": "0.11.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --names 'svelte,convex' --prefix-colors \"#f96743,#F3B01C\" \"vite dev --host\" \"pnpx convex dev\"",
    "dev:vite": "vite dev --host",
    "dev:convex": "pnpx convex dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  },
  "devDependencies": {
    "@ai-sdk/svelte": "^3.0.47",
    "@convex-dev/aggregate": "^0.1.24",
    "@convex-dev/better-auth": "^0.8.3",
    "@convex-dev/persistent-text-streaming": "^0.2.3",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.22.0",
    "@mmailaender/convex-better-auth-svelte": "^0.0.5",
    "@sveltejs/adapter-vercel": "^5.6.3",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@types/node": "^22",
    "@useautumn/convex": "^0.0.10",
    "ai": "^5.0.47",
    "autumn-js": "^0.1.35",
    "better-auth": "1.3.8",
    "concurrently": "^9.2.1",
    "convex": "^1.27.0",
    "convex-helpers": "^0.1.104",
    "convex-svelte": "^0.0.11",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "mdsvex": "^0.12.6",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "runed": "^0.32.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^7.0.4",
    "zod": "^3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
